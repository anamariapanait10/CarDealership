      "/orderfood": {
        "post": {
          "description": "Place an order for food in the restaurant.",
          "operationId": "orderFood",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["food_items", "payment_method", "customer_name", "phone_number"],
                  "properties": {
                    "food_items": { "type": "array", "items": { "type": "string" }, "description": "List of food items to order.", "x-custom-question": "What do you want to order?" },
                    "special_instructions": { "type": "string", "description": "Any special instructions for the order.", "x-custom-question": "Do you have any special instructions for your order?" },
                    "payment_method": { "type": "string", "enum": ["cash", "credit card", "debit card", "online payment"], "description": "Preferred payment method", "x-custom-question": "How would you like to pay for your order?" },
                    "customer_name": { "type": "string", "description": "Name of the customer placing the order", "x-custom-question": "What is the name for the order?" },
                    "phone_number": { "type": "string", "description": "Phone number for order confirmation", "x-custom-question": "Can I have your phone number to confirm the order?" },
                    "delivery_address": { "type": "string", "description": "Address for food delivery", "x-custom-question": "Where should we deliver your order?" },
                    "pricerange": { "type": "string", "enum": ["cheap", "moderate", "expensive"], "description": "Preferred price range for the food order.", "x-value": "moderate", "x-custom-question": "What is your preferred price range for the food?" },
                    "food": { "type": "string", "enum": ["italian", "vegetarian", "mexican", "chinese", "japanese"], "description": "Type of cuisine you are ordering.", "x-value": "italian", "x-custom-question": "What type of cuisine are you ordering?" }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful order placement.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "order_id": { "type": "string" },
                      "status": { "type": "string" },
                      "total_price": { "type": "number" },
                      "order_details": { "type": "array", "items": { "type": "string" } }
                    },
                    "example": {
                      "order_id": "67890",
                      "status": "confirmed",
                      "total_price": 42.99,
                      "order_details": ["1x Spaghetti Carbonara", "2x Margherita Pizza"]
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid order request.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": { "error": { "type": "string" } },
                    "example": { "error": "Invalid quantity for food items." }
                  }
                }
              }
            }
          }
        }
      }
