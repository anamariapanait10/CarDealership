    "paths": {
      "/bookrestaurant": {
        "post": {
          "description": "Make the restaurant reservation",
          "operationId": "bookRestaurant",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["name", "phone", "smoking", "date", "time", "diners", "pricerange"],
                  "properties": {
                    "name": { "type": "string", "description": "customer name", "x-custom-question": "What is the name of the reservation customer?" },
                    "phone": { "type": "string", "description": "phone number of the customer.", "x-custom-question": "Tell me your phone number in order to confirm the reservation, please." },
                    "smoking": { "type": "boolean", "description": "smoking zone", "x-custom-question": "do you want to eat in a smoking zone?" },
                    "date": { "type": "string", "description": "Date of the booking.", "x-custom-question": "What day do you want the reservation for?" },
                    "time": { "type": "string", "description": "Time of the booking.", "x-custom-question": "What time do you want the reservation for?" },
                    "diners": { "type": "integer", "description": "people for the booking", "x-custom-question": "For how many diners is the booking?" },
                    "location": {
                      "type": "string",
                      "description": "terrace or salon",
                      "enum": ["terrace", "salon"],
                      "x-custom-question": "Which location do you prefer, salon or terrace?"
                    },
                    "pricerange": {
                      "type": "string",
                      "description": "pricerange desired by the user",
                      "enum": ["moderate", "expensive", "cheap"],
                      "x-value": "moderate",
                      "x-custom-question": "What is the pricerange of the restaurant you are looking for?"
                    },
                    "food": {
                      "type": "string",
                      "description": "type of food desired by the user",
                      "enum": [
                        "afghan","african","afternoon tea","asian oriental","australasian","australian","austrian",
                        "barbeque","basque","belgian","bistro","brazilian","british","canapes","cantonese","caribbean",
                        "catalan","chinese","christmas","corsica","creative","crossover","cuban","danish","eastern european",
                        "english","eritrean","european","french","fusion","gastropub","german","greek","halal","hungarian",
                        "indian","indonesian","international","irish","italian","jamaican","japanese","korean","kosher",
                        "latin american","lebanese","light bites","malaysian","mediterranean","mexican","middle eastern",
                        "modern american","modern eclectic","modern european","modern global","molecular gastronomy","moroccan",
                        "new zealand","north african","north american","north indian","northern european","panasian","persian",
                        "polish","polynesian","portuguese","romanian","russian","scandinavian","scottish","seafood",
                        "singaporean","south african","south indian","spanish","sri lankan","steakhouse","swedish","swiss",
                        "thai","the americas","traditional","turkish","tuscan","unusual","vegetarian","venetian","vietnamese",
                        "welsh","world"
                      ],
                      "x-value": "vegetarian",
                      "x-custom-question": "Which is the kind of food you want to eat?"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful request",
              "content": {
                "application/json": {
                  "schema": { "type": "string", "example": "{\"message\":\"The phone number is: 675151146\"}" }
                }
              }
            },
            "404": {
              "description": "url not found",
              "content": {
                "application/json": {
                  "schema": { "type": "string", "example": "{\"message\":\"Error: The url or request is not found\"}" }
                }
              }
            }
          }
        }
      },
      "/restaurantinformation": {
        "get": {
          "description": "Retrieve general information about restaurants",
          "parameters": [
            {
              "name": "food",
              "in": "query",
              "description": "Type of food served at the restaurant",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": { "type": "string", "enum": ["italian","vegetarian","chinese","japanese","indian","french","mexican","thai","spanish"] },
              "x-value": "italian",
              "x-custom-question": "What type of food are you looking for at the restaurant?"
            },
            {
              "name": "pricerange",
              "in": "query",
              "description": "Price range of the restaurant",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": { "type": "string", "enum": ["cheap","moderate","expensive","luxury"] },
              "x-value": "expensive",
              "x-custom-question": "What is the price range you are looking for?"
            },
            {
              "name": "location",
              "in": "query",
              "description": "Physical location of the restaurant",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": { "type": "string" },
              "x-custom-question": "Where is the restaurant located?"
            },
            {
              "name": "rating",
              "in": "query",
              "description": "Customer rating of the restaurant (1 to 5 stars)",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": { "type": "integer", "minimum": 1, "maximum": 5 },
              "x-custom-question": "What rating are you looking for in a restaurant?"
            },
            {
              "name": "opening_hours",
              "in": "query",
              "description": "Hours of operation for the restaurant",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": { "type": "string" },
              "x-custom-question": "Do you want to know the restaurant's opening hours?"
            }
          ],
          "responses": {
            "200": {
              "description": "Successful request",
              "content": {
                "application/json": { "schema": { "type": "object" } }
              }
            }
          }
        }
      }
    },
    "components": {}
