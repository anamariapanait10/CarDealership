    },
    "openapi": "3.0.0",
    "info": {
      "title": "Car Test Drive Booking API",
      "description": "Car dealership test-drive booking for IA slot filling",
      "termsOfService": "http://miapp.com/terms.php",
      "contact": {
        "name": "María Jesús Rodríguez",
        "url": "https://directorio.ugr.es/static/PersonalUGR/*/show/8cbe900fb866f9ac9f399915a132dad3",
        "email": "mjesusrodriguez@ugr.es"
      },
      "license": {
        "name": "Apache 2.0",
        "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
      },
      "version": "1.0",
      "x-domain": "car_dealership"
    },
    "servers": [
      {
        "url": "http://127.0.0.1:5101",
        "description": "Server for the app"
      }
    ],
    "tags": [
      {
        "name": "testdrive, financing"
      }
    ],
    "paths": {
      "/scheduletestdrive": {
        "get": {
          "description": "Schedule a test drive at the dealership",
          "operationId": "scheduleTestDrive",
          "parameters": [
            {
              "name": "name",
              "in": "query",
              "description": "customer name",
              "required": true,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string"
              },
              "x-custom-question": "What is the name of the customer scheduling the test drive?"
            },
            {
              "name": "phone",
              "in": "query",
              "description": "phone number of the customer.",
              "required": true,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string"
              },
              "x-custom-question": "Tell me your phone number so we can confirm the test drive, please."
            },
            {
              "name": "date",
              "in": "query",
              "description": "Date of the test drive.",
              "required": true,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "format": "date"
              },
              "x-custom-question": "What day would you like to schedule the test drive?"
            },
            {
              "name": "time",
              "in": "query",
              "description": "Time of the test drive.",
              "required": true,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string"
              },
              "x-custom-question": "What time would you like the test drive?"
            },
            {
              "name": "make",
              "in": "query",
              "description": "Vehicle make desired by the user",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "enum": [
                  "toyota",
                  "bmw",
                  "tesla",
                  "ford",
                  "hyundai",
                  "volkswagen"
                ],
                "x-value": "toyota"
              },
              "x-custom-question": "Which car brand are you interested in?"
            },
            {
              "name": "model",
              "in": "query",
              "description": "Vehicle model desired by the user",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "enum": [
                  "corolla",
                  "camry",
                  "x5",
                  "model 3",
                  "mustang",
                  "tucson"
                ],
                "x-value": "corolla"
              },
              "x-custom-question": "Which model would you like to test drive?"
            },
            {
              "name": "condition",
              "in": "query",
              "description": "new, used, or certified pre-owned",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "enum": [
                  "new",
                  "used",
                  "certified pre-owned"
                ],
                "x-value": "new"
              },
              "x-custom-question": "Do you want to test drive a new, used, or certified pre-owned vehicle?"
            },
            {
              "name": "transmission",
              "in": "query",
              "description": "Preferred transmission",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "enum": [
                  "automatic",
                  "manual"
                ],
                "x-value": "automatic"
              },
              "x-custom-question": "Do you prefer automatic or manual transmission?"
            },
            {
              "name": "location",
              "in": "query",
              "description": "Preferred dealership location/branch",
              "required": false,
              "style": "form",
              "explode": true,
              "schema": {
                "type": "string",
                "enum": [
                  "main showroom",
                  "service lot",
                  "downtown branch"
                ],
                "x-value": "main showroom"
              },
              "x-custom-question": "Which branch do you prefer for the appointment?"
            }
          ],
          "responses": {
            "200": {
              "description": "Successful request",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "string",
                    "example": "{\"message\":\"Your test drive is confirmed.\"}"
                  }
                }
              }
            },
            "404": {
              "description": "url not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "string",
                    "example": "{\"message\":\"Error: The url or request is not found\"}"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {}
  },
  {
    "_id": {
